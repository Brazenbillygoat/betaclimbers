<!DOCTYPE html>
<html>
<body>

<%= render partial: "partials/navbar" %> 
  
<div class="community-body">
  <h1>Other Members in the <%=@community.name%> Community:</h1>
  <%@community.climbers.each do |climber|%> 
    <li><%=link_to climber.name, climber_path(climber)%></li>
    <%end%>
  <br><br>
  <div class="community-post-box">
  <h1> Posts </h1>
  </div>
    <div class="community-content-box">
      <% @community.posts.each do |post| %> 
        <h3><%= link_to post.subject, post_path(post) %></h3>
        <%end%>
        <br><br>
      <button class="ui standard button">
      <%= link_to "Create New Post", new_post_path, method: "get" %> 
      </button>
    </div>
  </div>  
</html>
</body>